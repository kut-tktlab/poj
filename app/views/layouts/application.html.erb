<!DOCTYPE html>
<html>
<head>
  <title>Poj</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <ol class="breadcrumb">
      <%= render_breadcrumbs :tag => :li, :separator => "" %>
    </ol>
    <%= yield %>
  </div>

  <script id="solution-status-label" type="x-template">
    <span :class="['label', labelClass]" v-text="status"></span>
  </script>
</body>
</html>
